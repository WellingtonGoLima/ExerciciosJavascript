class Pessoa {
    constructor(nome) {
        this.nome = 'Haroldinho'
    }
    falar(){
        console.log(`Meu nome é ${this.nome}`)

    }
}

const p1 = new Pessoa('Haroldinho')
p1.falar()

const criarPessoa = nome => {
    return{
        falar: () => console.log(`Meu nome é ${nome}`)
    }
}

const p2 = criarPessoa('Jão')
p2.falar()